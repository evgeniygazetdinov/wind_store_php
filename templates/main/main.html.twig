        {% extends "base/base.html.twig"%}

        {% block title %}Welcome!{% endblock %}
        {% block reklama%}
        {% include '/main/main_title.html.twig' %}
        {% endblock %}
        {% block main_title %}

        {% endblock %}
        {% block body %}
        <div class="main-title-block">
          <p>{{main_title}}</p>
        </div>
        <div class="generators">
          {% for generator in generators%}
              <div class="generator-for-sale">
                  {% include '/catalog/generator.html.twig' %}
              </div>
            {% endfor %}

        </div>
        <div class="all-in-catalog">
          <a href="{{ path('catalog')}}">Весь каталог</a>
        </div>
        <div class="main-title-block main-title-news">
          <div class="news service">
            {%for news in all_news%}
            {% include '/news/news.html.twig'%}
            {% endfor%}
          </div>

        </div>
        <div class="all-in-catalog">
          <a href="{{ path('news')}}">Весь новости</a>
        </div>
        {% endblock %}
        {% block javascripts %}{% endblock %}
